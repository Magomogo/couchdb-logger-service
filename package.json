        "underscore": "~1.4.4",
        "mime": "~1.2.11",
        "request": "~2.27.0",
        "browserify": "~2.34.1",
        "mustache": "~0.7.2",
        "jquery-commonjs": "~1.7.1",
        "html5-history": "~0.1.5",
        "moment": "2.3.1"
